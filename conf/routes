# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /												controllers.Application.index()
GET		/layersall/:serviceId							controllers.Application.allLayers(serviceId: String)
GET		/layer/:serviceId/:layerId						controllers.Application.layers(serviceId: String, layerId: String)
GET		/routes.js										controllers.Application.jsRoutes()

GET		/proxy/*url										controllers.Proxy.proxy(url: String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               					controllers.Assets.versioned(path="/public", file: Asset)